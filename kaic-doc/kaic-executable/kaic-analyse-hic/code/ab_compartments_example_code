
# start snippet compartments basic
kaic compartments output/hic/binned/kaic_example_1mb.hic architecture/compartments/kaic_example_1mb.ab
# end snippet compartments basic

# start snippet compartments plot
klot -o architecture/compartments/kaic_example_1mb.ab.png chr18 \
     -p square architecture/compartments/kaic_example_1mb.ab \
     -vmin -0.75 -vmax 0.75 -c RdBu_r
# end snippet compartments plot

klot -o ../kaic-doc/kaic-executable/kaic-analyse-hic/images/kaic_example_1mb.ab.png chr18 \
     -p square architecture/compartments/kaic_example_1mb.ab \
     -vmin -0.75 -vmax 0.75 -c RdBu_r


# start snippet compartments ev
kaic compartments -v architecture/compartments/kaic_example_1mb.ev.txt \
                  output/hic/binned/kaic_example_1mb.hic \
                  architecture/compartments/kaic_example_1mb.ab
# end start snippet compartments ev

