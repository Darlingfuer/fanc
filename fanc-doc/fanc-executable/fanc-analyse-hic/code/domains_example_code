# start snippet domains basic
klot -o architecture/domains/kaic_example_100kb_tads.png chr18:18mb-28mb \
     -p triangular output/hic/binned/kaic_example_100kb.hic -m 4000000 \
     -vmin 0 -vmax 0.05
# end snippet domains basic
klot -o ../kaic-doc/kaic-executable/kaic-analyse-hic/images/kaic_example_100kb_tads.png chr18:18mb-28mb \
     -p triangular output/hic/binned/kaic_example_100kb.hic -m 4000000 \
     -vmin 0 -vmax 0.05

# start snippet domains window
kaic insulation output/hic/binned/kaic_example_100kb.hic \
                architecture/domains/kaic_example_100kb.insulation \
                -w 1000000 1500000 2000000 2500000 3000000 3500000 4000000
# end snippet domains window

# start snippet domains scores
klot -o architecture/domains/kaic_example_50kb_tads_insulation.png chr18:18mb-28mb \
     -p triangular output/hic/binned/kaic_example_100kb.hic -m 4000000 -vmin 0 -vmax 0.05 \
     -p scores architecture/domains/kaic_example_100kb.insulation
# end snippet domains scores
klot -o ../kaic-doc/kaic-executable/kaic-analyse-hic/images/kaic_example_50kb_tads_insulation.png chr18:18mb-28mb \
     -p triangular output/hic/binned/kaic_example_100kb.hic -m 4000000 -vmin 0 -vmax 0.05 \
     -p scores architecture/domains/kaic_example_100kb.insulation

# start snippet domains bed
kaic insulation output/hic/binned/kaic_example_100kb.hic \
                architecture/domains/kaic_example_100kb.insulation \
                -w 1000000 1500000 2000000 2500000 3000000 3500000 4000000 \
                -o bed
# end snippet domains bed

