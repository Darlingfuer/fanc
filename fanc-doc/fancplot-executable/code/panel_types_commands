# start snippet triangular basic
fancplot chr18:6mb-10mb -p triangular output/hic/binned/fanc_example_50kb.hic
# end snippet triangular basic

# start snippet triangular truncated
fancplot chr18:6mb-10mb -p triangular -m 2mb output/hic/binned/fanc_example_50kb.hic
# end snippet triangular truncated
fancplot -o ../fanc-doc/fancplot-executable/images/fancplot_triangular_example_truncated.png chr18:6mb-10mb -p triangular -m 2mb output/hic/binned/fanc_example_50kb.hic

# start snippet triangular range
fancplot chr18:6mb-10mb -p triangular -r output/hic/binned/fanc_example_50kb.hic
# end snippet triangular range
fancplot -o ../fanc-doc/fancplot-executable/images/fancplot_triangular_example_range.png chr18:6mb-10mb -p triangular -r output/hic/binned/fanc_example_50kb.hic

# start snippet triangular vminvmax
fancplot chr18:6mb-10mb -p triangular -vmin 0 -vmax 0.03 output/hic/binned/fanc_example_50kb.hic
# end snippet triangular vminvmax
fancplot -o ../fanc-doc/fancplot-executable/images/fancplot_triangular_example_vminvmax.png chr18:6mb-10mb -p triangular -vmin 0 -vmax 0.03 output/hic/binned/fanc_example_50kb.hic

# start snippet triangular log
fancplot chr18:6mb-10mb -p triangular -l output/hic/binned/fanc_example_50kb.hic
# end snippet triangular log
fancplot -o ../fanc-doc/fancplot-executable/images/fancplot_triangular_example_log.png chr18:6mb-10mb -p triangular -l output/hic/binned/fanc_example_50kb.hic

# start snippet triangular colormap
fancplot chr18:6mb-10mb -p triangular -c Greens output/hic/binned/fanc_example_50kb.hic
# end snippet triangular colormap
fancplot -o ../fanc-doc/fancplot-executable/images/fancplot_triangular_example_colormap.png chr18:6mb-10mb -p triangular -c Greens output/hic/binned/fanc_example_50kb.hic


