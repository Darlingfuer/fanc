# start snippet square basic
fancplot chr18:6mb-10mb -p square output/hic/binned/fanc_example_50kb.hic
# end snippet square basic
fancplot -o ../fanc-doc/fancplot-executable/images/fancplot_square_example.png chr18:6mb-10mb -p square output/hic/binned/fanc_example_50kb.hic

# start snippet square flip
fancplot chr18:6mb-10mb -p square -f output/hic/binned/fanc_example_50kb.hic
# end snippet square flip
fancplot -o ../fanc-doc/fancplot-executable/images/fancplot_square_example_flip.png chr18:6mb-10mb -p square -f output/hic/binned/fanc_example_50kb.hic

# start snippet mirror basic
fancplot chr18:6mb-10mb -p mirror -d 3mb output/hic/binned/fanc_example_50kb.hic output/hic/binned/fanc_example_100kb.hic
# end snippet mirror basic
fancplot -o ../fanc-doc/fancplot-executable/images/fancplot_square_example_mirror.png chr18:6mb-10mb -p mirror -d 3mb output/hic/binned/fanc_example_50kb.hic output/hic/binned/fanc_example_100kb.hic

# start snippet mirror oe
fancplot chr18:6mb-10mb -p mirror -d 3mb -le -lvmin -2 -lvmax 2 output/hic/binned/fanc_example_50kb.hic output/hic/binned/fanc_example_50kb.hic
# end snippet mirror oe
fancplot -o ../fanc-doc/fancplot-executable/images/fancplot_square_example_mirror_oe.png chr18:6mb-10mb -p mirror -d 3mb -le -lvmin -2 -lvmax 2 output/hic/binned/fanc_example_50kb.hic output/hic/binned/fanc_example_50kb.hic


