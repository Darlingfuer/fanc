fancplot chr18:6mb-10mb -p triangular -u output/hic/binned/fanc_example_50kb.hic
# end snippet triangular nonorm
fancplot -o ../fanc-doc/fancplot-executable/images/fancplot_triangular_example_nonorm.png chr18:6mb-10mb -p triangular -u output/hic/binned/fanc_example_50kb.hic

